	"styles": {
		"color": {
			"background": "var(--wp--preset--color--base)",
			"text": "var(--wp--preset--color--contrast)"
		},
		"typography": {
            "fontFamily": "var(--wp--preset--font-family--merriweather)",
            "lineHeight": "1.55"
        },
		"elements": {
			"button": {
				":active": {
					"color": {
						"background": "var(--wp--preset--color--contrast)",
						"text": "var(--wp--preset--color--base)"
					}
				},
				":focus": {
					"color": {
						"background": "var(--wp--preset--color--contrast-2)",
						"text": "var(--wp--preset--color--base)"
					},
					"outline": {
						"color": "var(--wp--preset--color--contrast)",
						"offset": "2px"
					},
					"border": {
						"color": "var(--wp--preset--color--contrast-2)"
					}
				},
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--contrast-2)",
						"text": "var(--wp--preset--color--base)"
					},
					"border": {
						"color": "var(--wp--preset--color--contrast-2)"
					}
				},
				"border": {
					"radius": ".33rem",
					"color": "var(--wp--preset--color--contrast)"
				},
				"color": {
					"background": "var(--wp--preset--color--contrast)",
					"text": "var(--wp--preset--color--base)"
				},
				"spacing": {
					"padding": {
						"bottom": "0.6rem",
						"left": "1rem",
						"right": "1rem",
						"top": "0.6rem"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontStyle": "normal",
					"fontWeight": "500"
				}
			},
			"heading": {
				"typography": {
                    "fontFamily": "var(--wp--preset--font-family--oswald)",
                    "textTransform": "uppercase",
                    "fontStyle": "normal",
                    "fontWeight": "400"
                }
			},
			"h1": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--oswald)",
					"fontSize": "var(--wp--preset--font-size--xx-large)",
					"lineHeight": "1.15"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--oswald)",
					"fontSize": "var(--wp--preset--font-size--x-large)"
				}
			},
			"h3": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--merriweather)",
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontStyle": "italic",
					"fontWeight": "400",
					"textTransform": "none"
				},
				"css": "color: var(--wp--preset--color--bulldog-red)"
			},
			"h4": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--oswald)",
					"fontSize": "clamp(1.1rem, 1.1rem + ((1vw - 0.2rem) * 0.767), 1.5rem)"
				}
			},
			"h5": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--oswald)",
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"h6": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--oswald)",
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--bulldog-red)"
				}
			}
		},
		"blocks": {
			"core/button": {
                "border": {
                    "radius": ".33rem"
                },
                "color": {
                    "background": "var(--wp--preset--color--bulldog-red)",
                    "text": "var(--wp--preset--color--base)"
                },
                "typography": {
                    "lineHeight": "1"
                },
                "spacing": {
                    "padding": {
                        "top": "var(--wp--preset--spacing--30)",
                        "bottom": "var(--wp--preset--spacing--30)",
                        "left": "var(--wp--preset--spacing--40)",
                        "right": "var(--wp--preset--spacing--40)"
                    }
                },
                "elements": {
                    "link": {
                        "color": {
                            "text": "var(--wp--preset--color--base)"
                        }
                    }
                }
            },
			"core/separator": {
                "border": {
                    "color": "var(--wp--preset--color--gray)",
                    "width": "1px"
                }
            }
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		},
		{
			"area": "uncategorized",
			"name": "sidebar",
			"title": "Sidebar"
		}
	]
