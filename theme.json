	"styles": {
		"blocks": {
			"core/button": {
				"border": {
					"radius": ".33rem"
				},
				"color": {
					"background": "var(--wp--preset--color--olympic)",
					"text": "var(--wp--preset--color--base)"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--base)"
						}
					}
				},
				"spacing": {
					"padding": {
						"bottom": "var(--wp--preset--spacing--30)",
						"left": "var(--wp--preset--spacing--40)",
						"right": "var(--wp--preset--spacing--40)",
						"top": "var(--wp--preset--spacing--30)"
					}
				},
				"typography": {
					"lineHeight": "1"
				}
			},
			"core/separator": {
				"border": {
					"color": "var(--wp--preset--color--gray)",
					"width": "1px"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--base)",
			"text": "var(--wp--preset--color--contrast)"
		},
		"elements": {
			"button": {
				":active": {
					"color": {
						"background": "var(--wp--preset--color--contrast)",
						"text": "var(--wp--preset--color--base)"
					}
				},
				":focus": {
					"border": {
						"color": "var(--wp--preset--color--contrast-2)"
					},
					"color": {
						"background": "var(--wp--preset--color--contrast-2)",
						"text": "var(--wp--preset--color--base)"
					},
					"outline": {
						"color": "var(--wp--preset--color--contrast)",
						"offset": "2px"
					}
				},
				":hover": {
					"border": {
						"color": "var(--wp--preset--color--contrast-2)"
					},
					"color": {
						"background": "var(--wp--preset--color--contrast-2)",
						"text": "var(--wp--preset--color--base)"
					}
				},
				"border": {
					"color": "var(--wp--preset--color--contrast)",
					"radius": ".33rem"
				},
				"color": {
					"background": "var(--wp--preset--color--olympic)",
					"text": "var(--wp--preset--color--base)"
				},
				"spacing": {
					"padding": {
						"bottom": "0.6rem",
						"left": "1rem",
						"right": "1rem",
						"top": "0.6rem"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontStyle": "normal",
					"fontWeight": "500"
				}
			},
			"h1": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--oswald)",
					"fontSize": "67px",
					"fontStyle": "normal",
					"fontWeight": "700",
					"lineHeight": "1.15",
					"textTransform": "uppercase"
				}
			},
			"h2": {
				"css": "color: var(--wp--preset--color--bulldog-red)",
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--oswald)",
					"fontSize": "3rem",
					"textTransform": "uppercase"
				}
			},
			"h3": {
